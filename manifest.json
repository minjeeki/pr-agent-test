{
  "manifest_version": 3,
  "name": "Catify",
  "description": "Choose a cat image to set as a background for all the pages you visit",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon16.png",
    "default_title": "Catify",
    "default_popup": "popup/popup.html"
  }
}
